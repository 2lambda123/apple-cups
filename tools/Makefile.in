# Programs and options...
CC		=	@CC@
CFLAGS		=	@CFLAGS@
LIBS		=	@LIBS@
SHELL		=	/bin/sh


# Directories...
prefix		=	@prefix@
exec_prefix	=	@exec_prefix@
bindir		=	@bindir@


# Targets...
OBJS		=	sendbuildlog.o
TARGETS		=	sendbuildlog


# Make everything...
all:	$(TARGETS)


# Clean everything...
clean:
	rm -f $(TARGETS) $(OBJS)


# Really clean everything...
distclean:	clean
	rm -f Makefile config.log config.status


# Install everything...
install: all
	-mkdir -p $(bindir)
	cp sendbuildlog $(bindir)


# sendbuildlog mail tool
sendbuildlog:	sendbuildlog.o
	$(CC) $(CFLAGS) -o $@ sendbuildlog.o $(LIBS)

